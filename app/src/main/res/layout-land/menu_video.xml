<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:weightSum="18"
    android:background="@drawable/img_menu_inicio"
    android:orientation="vertical"
    tools:context="appkite.jordiguzman.com.bichosfeos.MenuVideo">

    <fragment
        android:id="@+id/fragment_pulga"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_video.Frag_pulga"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3"
        tools:layout="@layout/frag_pulga" />
    <fragment
        android:id="@+id/fragment_piojo"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_video.Frag_piojo"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3"
        tools:layout="@layout/frag_piojo" />
    <fragment
        android:id="@+id/fragment_mosca"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_video.Frag_mosca"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3"
        tools:layout="@layout/frag_mosca" />
    <fragment
        android:id="@+id/fragment_mosquito"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_video.Frag_mosquito"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3"
        tools:layout="@layout/frag_mosquito" />
    <fragment
        android:id="@+id/fragment_varios"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_video.Frag_imagenes"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3"
        tools:layout="@layout/frag_imagenes_vari" />
    <fragment
        android:id="@+id/fragment_varios2"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_video.Frag_imagenes2"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3"
        tools:layout="@layout/frag_imagenes_vari2" />

</LinearLayout>
