<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_inicio"
    android:background="@drawable/img_menu_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:weightSum="18"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="appkite.jordiguzman.com.bichosfeos.MenuInfo">

    <fragment
        android:id="@+id/fragment_img"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_info.Frag_teoria"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_inf"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_info.Frag_tecnica"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_enlaces"
        class="appkite.jordiguzman.com.bichosfeos.fragments.fragment_info.Frag_info_img"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />



</LinearLayout>