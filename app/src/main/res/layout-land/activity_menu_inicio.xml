<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_inicio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img_menu_video"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="appkite.jordiguzman.com.bichosfeos.MenuInicio">

    <fragment
        android:id="@+id/fragment_img"
        class="appkite.jordiguzman.com.bichosfeos.fragments.frag_menu_inicio.Frag_imagenes"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_inf"
        class="appkite.jordiguzman.com.bichosfeos.fragments.frag_menu_inicio.Frag_info"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_enlaces"
        class="appkite.jordiguzman.com.bichosfeos.fragments.frag_menu_inicio.Frag_enlaces"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_video"
        class="appkite.jordiguzman.com.bichosfeos.fragments.frag_menu_inicio.Frag_video"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_idioma"
        class="appkite.jordiguzman.com.bichosfeos.fragments.frag_menu_inicio.Frag_idiomas"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />

    <fragment
        android:id="@+id/fragment_acerca"
        class="appkite.jordiguzman.com.bichosfeos.fragments.frag_menu_inicio.Frag_acerca"
        android:layout_width="match_parent"
        android:layout_height="0sp"
        android:layout_weight="3" />



</LinearLayout>
